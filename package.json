{
  "name": "nest-hybrid-architecture",
  "version": "0.0.1",
  "description": "Nest hybrid architecture",
  "author": "Company.x",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.10.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "ci": "yarn install --frozen-lockfile",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "seed": "NODE_ENV=development ts-node src/seeder.ts --watch --config seeder-cli.json",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=development jest",
    "test:watch": "NODE_ENV=development jest --watch",
    "test:cov": "NODE_ENV=development jest --coverage",
    "test:debug": "NODE_ENV=development node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json"
  },
  "dependencies": {
    "@faker-js/faker": "^7.1.0",
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.5",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/typeorm": "^8.1.4",
    "argon2": "^0.28.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "class-validator-multi-lang": "^0.130.201",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "dotenv-flow": "^3.2.0",
    "express-session": "^1.17.3",
    "generate-password": "^1.7.0",
    "iterare": "^1.2.1",
    "lodash.isstring": "^4.0.1",
    "lodash.merge": "^4.6.2",
    "md5": "^2.3.0",
    "nestjs-i18n": "^9.1.0",
    "nestjs-real-ip": "^2.1.1",
    "nestjs-redoc": "^2.2.2",
    "node-mailjet": "^5.0.1",
    "numeral": "^2.0.6",
    "parse-num": "^1.0.0",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-headerapikey": "^1.2.2",
    "passport-instagram": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-okta-oauth20": "^1.1.0",
    "passport-twitch-latest": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express-session": "^1.17.4",
    "@types/jest": "27.5.0",
    "@types/lodash.isarray": "^4.0.7",
    "@types/lodash.isstring": "^4.0.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/md5": "^2.3.2",
    "@types/node": "^16.0.0",
    "@types/node-mailjet": "^3.3.9",
    "@types/numeral": "^2.0.2",
    "@types/passport-discord": "^0.1.5",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-github": "^1.1.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-instagram": "^1.0.2",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-linkedin-oauth2": "^1.5.3",
    "@types/passport-local": "^1.0.34",
    "@types/passport-twitch-latest": "^1.0.1",
    "@types/passport-twitter": "^1.0.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
